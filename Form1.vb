    Sub CreateTextFile(ByVal filePath As String, ByVal content As String)
        ' Check if the file exists
        If File.Exists(filePath) Then
            Try
                ' Delete the existing file
                File.Delete(filePath)
                Console.WriteLine("Existing file deleted.")
            Catch ex As Exception
                Console.WriteLine("An error occurred while deleting the file: " & ex.Message)
                Return
            End Try
        End If

        ' Create the file and write the content
        Try
            File.WriteAllText(filePath, content)
            Console.WriteLine("File created and content written.")
        Catch ex As Exception
            Console.WriteLine("An error occurred while writing to the file: " & ex.Message)
        End Try
    End Sub
    Function ReadTextFile(ByVal filePath As String) As String
        ' Check if the file exists
        If File.Exists(filePath) Then
            Try
                ' Read all the content from the file
                Dim content As String = File.ReadAllText(filePath)
                Return content
            Catch ex As Exception
                Console.WriteLine("An error occurred: " & ex.Message)
                Return String.Empty
            End Try
        Else
            Console.WriteLine("File does not exist.")
            Return String.Empty
        End If
    End Function

