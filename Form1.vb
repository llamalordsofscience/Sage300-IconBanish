    Public Function FindRowInArray(sage300Modules_APVENDORS_HEX(,) As String, dataGridView As DataGridView) As List(Of Integer)
        Dim foundRows As New List(Of Integer)()

        ' Check if the DataGridView has any rows
        If dataGridView IsNot Nothing AndAlso dataGridView.Rows.Count > 0 Then
            ' Loop through each row in the DataGridView
            For Each row As DataGridViewRow In dataGridView.Rows
                ' Ensure the row is not a new row and has cells
                If Not row.IsNewRow AndAlso row.Cells.Count > 0 Then
                    ' Check if the cell value is not null
                    If row.Cells(0).Value IsNot Nothing Then
                        Dim searchString As String = row.Cells(0).Value.ToString()

                        ' Search for the string in the first column of the 2D array
                        For i As Integer = 0 To sage300Modules_APVENDORS_HEX.GetLength(0) - 1
                            If sage300Modules_APVENDORS_HEX(i, 0).Equals(searchString, StringComparison.OrdinalIgnoreCase) Then
                                foundRows.Add(i) ' Add the row index to the list
                                Exit For ' Exit the inner loop once a match is found
                            End If
                        Next
                    End If
                End If
            Next
        End If

        Return foundRows
    End Function 'returns the indexes of the array the user wants to update. 

    Public Sub RemoveHexValueFromBinaryFile(filePath As String, sage300Modules_(,) As String, index As Integer)
        Try
            ' Validate the index
            If index < 0 OrElse index >= sage300Modules_.GetLength(0) Then
                Throw New ArgumentOutOfRangeException("Index is out of range.")
            End If

            ' Read all bytes from the binary file
            Dim fileBytes As Byte() = File.ReadAllBytes(filePath)

            ' Get the hex value to remove from the specified index
            Dim hexValue As String = sage300Modules_(index, 1)

            ' Convert hex string to byte array
            Dim hexBytes As Byte() = ConvertHexStringToByteArray(hexValue)

            ' Remove occurrences of the hex byte array from the file bytes
            fileBytes = RemoveByteArray(fileBytes, hexBytes)

            ' Write the modified bytes back to the binary file
            File.WriteAllBytes(filePath, fileBytes)
        Catch ex As Exception
            MsgBox("Failed to update: " + ex.Message)
        End Try
    End Sub 'removes the binary data / hex data from the grp file. 

    Private Function ConvertHexStringToByteArray(hexString As String) As Byte()
        ' Split the hex string by spaces and convert to byte array
        Dim hexValues As String() = hexString.Split(New Char() {" "c}, StringSplitOptions.RemoveEmptyEntries)
        Dim bytes As Byte() = New Byte(hexValues.Length - 1) {}

        For i As Integer = 0 To hexValues.Length - 1
            bytes(i) = Convert.ToByte(hexValues(i), 16)
        Next

        Return bytes
    End Function 'conversion 

    Private Function RemoveByteArray(source As Byte(), toRemove As Byte()) As Byte()
        Dim result As New List(Of Byte)()
        Dim i As Integer = 0

        While i < source.Length
            ' Check if the current position matches the byte array to remove
            If source.Skip(i).Take(toRemove.Length).SequenceEqual(toRemove) Then
                i += toRemove.Length ' Skip the bytes to remove
            Else
                result.Add(source(i)) ' Add the current byte to the result
                i += 1
            End If
        End While

        Return result.ToArray()
    End Function 'removal functional 

