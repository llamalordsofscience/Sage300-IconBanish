    Private Function CopyFile(sourceFilePath As String, destinationFolder As String, Optional newFolderName As String = "") As Boolean
        Try
            ' Ensure the source file exists
            If Not File.Exists(sourceFilePath) Then
                'MessageBox.Show("Source file does not exist. :" + newFolderName)
                Return False
            End If

            ' If a new folder name is provided, create the new folder in the destination folder
            If Not String.IsNullOrEmpty(newFolderName) Then
                destinationFolder = Path.Combine(destinationFolder, newFolderName)
                If Not Directory.Exists(destinationFolder) Then
                    Directory.CreateDirectory(destinationFolder)
                End If
            End If

            ' Ensure the destination folder exists
            If Not Directory.Exists(destinationFolder) Then
                Directory.CreateDirectory(destinationFolder)
            End If

            ' Get the file name from the source file path
            Dim fileName As String = Path.GetFileName(sourceFilePath)

            ' Define the destination file path
            Dim destFilePath As String = Path.Combine(destinationFolder, fileName)

            ' Copy the file to the destination folder
            File.Copy(sourceFilePath, destFilePath, True) ' Overwrite if the file already exists

            ' Return True indicating the file was copied successfully
            Return True
        Catch ex As Exception
            MessageBox.Show("An error occurred: " & ex.Message)
            Return False
        End Try
    End Function 'used to copy files from one area to another. 

