    Private Sub Form1_Load(sender As Object, e As EventArgs) Handles MyBase.Load

        MsgBox("WARNING: Please read the README.md file as well as the help and about information.")

        GUI_Setup()

        If CheckWaybackFolderExists() Then
            disableControls()
            Sage300_CustomPanel1.ToggleMenuItemVisibility("Setup", True)
        Else
            EnableControls()
            Sage300_CustomPanel1.ToggleMenuItemVisibility("Setup", False)

            Dim exePath As String = AppDomain.CurrentDomain.BaseDirectory
            Dim folderPath As String = Path.Combine(exePath, "WAYBACK")
            Dim filePath As String = Path.Combine(folderPath, "Ver.txt")
            SageFolderVersion = ReadTextFile(filePath)
            TextBox3.Text = SageFolderVersion
            ClearDataGridViews()
            GetSubData(CurrentIndex, SubCurrentIndex)
        End If
    End Sub 'handles the startup of the application

